{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "arrow-parens": true,
        "member-access": [true, "check-accessor"],
        "comment-format": [true, "check-space"],
        "no-console": false,
        "prefer-const": [true, { "destructuring": "all" }],
        "indent": [true, "spaces", 2],
        "quotemark": [true, "double"],
        "trailing-comma": [
            false,
            {
                "multiline": {
                    "objects": "never",
                    "arrays": "never",
                    "functions": "never"
                }
            }
        ]
    }
}
